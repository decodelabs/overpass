{
    "name": "decodelabs/overpass",
    "description": "Simple node.js bridge",
    "type": "library",
    "keywords": [ "node", "js", "bridge" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/atlas": "^0.12",
        "decodelabs/exceptional": "^0.5",
        "decodelabs/systemic": "^0.11.6",
        "decodelabs/terminus": "^0.11",
        "decodelabs/veneer": "^0.12.1"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Overpass\\": "src/"
        },
        "files": [
            "src/Context.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
