{
    "name": "decodelabs/overpass",
    "description": "Simple node.js bridge",
    "type": "library",
    "keywords": [ "node", "js", "bridge" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.3.10",
        "decodelabs/atlas": "^0.13.1",
        "decodelabs/exceptional": "^0.6.1",
        "decodelabs/monarch": "^0.1.4",
        "decodelabs/systemic": "^0.11.18"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Overpass\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
